type CvTag {
    id: String!
    jobId: String!
    createdBy: String!
    objectKey: String!
    downloadUrl: String!
    score: Float!
}

type Page {
    items: [CvTag]!
    nextCursor: String
    hasNextPage: Boolean!
}

type Query {
    search(
        jobId: String
        createdBy: String
        cursor: String
        limit: Int
    ): Page!
}